#map{
  position: relative;
  width: 924px;
  height: 464px;
  clear: both;
  background: #e5e3df;
}
.infoBox {
  background: #fff;
  border: 5px solid #e4e4e4;
  width: 220px;
  z-index: 9999;
  color: #666;
  padding: 20px;
}
.infoBox h3 {
  color: #243488;
  font-size: 18px;
  margin-top: 0;
  margin-bottom: 10px;
}
.infoBox h3 span {
  margin-right: 9px;
}
.infoBox p {
  color: #282828;
  font-size: 11px;
}
.infoBox h4 {
  color: #282828;
  font-size: 14px;
  margin-bottom: 5px;
  margin-top: 10px;
}
.infoBox .liens {
  margin-top: 10px;
  color: #c8bfa7;
  text-decoration: none;
  font-family:"HelveticaNeueW01-67MdCn 692710";
  font-size: 14px;
  padding-right: 20px;
}
.infoBox a.liens {
  background: url(images/puce.png) right center no-repeat;
}